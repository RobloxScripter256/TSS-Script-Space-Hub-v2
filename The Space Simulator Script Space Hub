centaurasesp.MouseButton1Click:Connect(function()
local centaurFolder = getCentaurFolder()
if not centaurFolder then
warn("Centaurs folder not found, can't toggle ESP.")
return
end

espOn = not espOn  
if espOn then  
	centaurasesp.Text = "Centaurs ESP: On"  
	addBillboards()  
else  
	centaurasesp.Text = "Centaurs ESP: Off"  
	removeBillboards()  
end

end)

local dialogGui = game.Players.LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("DialogGui")
local cleanerOn = false
local cleanerConnection = nil

UICorner_9.Parent = anticheatframehide

anticheatframehide.Name = "AntiCheatNotiHide"
anticheatframehide.Parent = Lists
anticheatframehide.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
anticheatframehide.BackgroundTransparency = 0.650
anticheatframehide.BorderColor3 = Color3.fromRGB(0, 0, 0)
anticheatframehide.BorderSizePixel = 0
anticheatframehide.Position = UDim2.new(0.373001873, 0, 0, 0)
anticheatframehide.Size = UDim2.new(0.99000001, 0, 0.0799999982, 0)
anticheatframehide.Font = Enum.Font.FredokaOne
anticheatframehide.Text = "Hide Anti-Cheat Frame: Off"
anticheatframehide.TextColor3 = Color3.fromRGB(255, 255, 255)
anticheatframehide.TextScaled = true
anticheatframehide.TextSize = 14.000
anticheatframehide.TextWrapped = true
anticheatframehide.MouseButton1Click:Connect(function()
cleanerOn = not cleanerOn

if cleanerOn then  
	anticheatframehide.Text = "Hide Anti-Cheat Frame: On"  

	-- Start cleaning loop  
	cleanerConnection = RunService.RenderStepped:Connect(function()  
		if not dialogGui then return end  

		local dialogsFrame = dialogGui:FindFirstChild("DialogsFrames")  
		if dialogsFrame then  
			for _, child in ipairs(dialogsFrame:GetChildren()) do  
				if child:IsA("Frame") then  
					child:Destroy()  
				end  
			end  
		end  
	end)  

else  
	anticheatframehide.Text = "Hide Anti-Cheat Frame: Off"  

	-- Stop cleaning loop  
	if cleanerConnection then  
		cleanerConnection:Disconnect()  
		cleanerConnection = nil  
	end  
end

end)

UICorner_10.Parent = tpvoayger

tpvoayger.Name = "TpVoyager"
tpvoayger.Parent = Lists
tpvoayger.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
tpvoayger.BackgroundTransparency = 0.650
tpvoayger.BorderColor3 = Color3.fromRGB(0, 0, 0)
tpvoayger.BorderSizePixel = 0
tpvoayger.Position = UDim2.new(0.373001873, 0, 0, 0)
tpvoayger.Size = UDim2.new(0.99000001, 0, 0.0799999982, 0)
tpvoayger.Font = Enum.Font.FredokaOne
tpvoayger.Text = "Loop Tp Voyager: Off"
tpvoayger.TextColor3 = Color3.fromRGB(255, 255, 255)
tpvoayger.TextScaled = true
tpvoayger.TextSize = 14.000
tpvoayger.TextWrapped = true

local espOn = false
local teleportLoop

local targetPart = workspace:WaitForChild("Planetoids")
:WaitForChild("Voyager")
:WaitForChild("Minerals")
:WaitForChild("Golden Record")

tpvoayger.Text = "Loop Tp Voyager: Off"

tpvoayger.MouseButton1Click:Connect(function()
espOn = not espOn
if espOn then
tpvoayger.Text = "Loop Tp Voyager: On"

teleportLoop = RunService.Heartbeat:Connect(function()  
		local character = player.Character  
		if character and character.PrimaryPart then  
			character:SetPrimaryPartCFrame(targetPart.CFrame + Vector3.new(0, 0, 0))  
		end  
	end)  

	-- Auto toggle off after 20 seconds  
	delay(20, function()  
		if espOn then  
			espOn = false  
			tpvoayger.Text = "Loop Tp Voyager: Off"  
			if teleportLoop then  
				teleportLoop:Disconnect()  
				teleportLoop = nil  
			end  
		end  
	end)  
else  
	tpvoayger.Text = "Loop Tp Voyager: Off"  
	if teleportLoop then  
		teleportLoop:Disconnect()  
		teleportLoop = nil  
	end  
end

end)
