ChangeSpeedOfShip.Name = "ChangeSpeedOfShip"
ChangeSpeedOfShip.Parent = Lists
ChangeSpeedOfShip.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
ChangeSpeedOfShip.BackgroundTransparency = 0.650
ChangeSpeedOfShip.BorderColor3 = Color3.fromRGB(0, 0, 0)
ChangeSpeedOfShip.BorderSizePixel = 0
ChangeSpeedOfShip.Position = UDim2.new(0.373001873, 0, 0, 0)
ChangeSpeedOfShip.Size = UDim2.new(0.99000001, 0, 0.0799999982, 0)
ChangeSpeedOfShip.Font = Enum.Font.FredokaOne
ChangeSpeedOfShip.Text = "Change Speed To Max 100 need sit on ship"
ChangeSpeedOfShip.TextColor3 = Color3.fromRGB(255, 255, 255)
ChangeSpeedOfShip.TextScaled = true
ChangeSpeedOfShip.TextSize = 14.000
ChangeSpeedOfShip.TextWrapped = true
ChangeSpeedOfShip.MouseButton1Click:Connect(function()
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:FindFirstChildOfClass("Humanoid")

if humanoid and humanoid.SeatPart then  
	local seatParent = humanoid.SeatPart.Parent  
	local main = seatParent:FindFirstChild("Main")  

	if main then  
		if main:GetAttribute("maxSpeed") ~= nil then  
			main:SetAttribute("maxSpeed", 100)  
			print("maxSpeed set to 100!")  
		else  
			warn("Main exists but 'maxSpeed' attribute not found.")  
		end  
	else  
		warn("Main not found under SeatPart's parent.")  
	end  
else  
	warn("You must be sitting on the ship to change speed.")  
end

end)

